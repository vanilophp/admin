{
    "name": "vanilo/admin",
    "description": "Admin Panel for the Vanilo E-commerce Framework",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/vanilophp/admin/issues"
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/framework": "^9.0",
        "konekt/appshell": "^3.0",
        "konekt/concord": "^1.11",
        "konekt/enum": "^3.1.1|^4.0",
        "spatie/laravel-medialibrary": "^10.0",
        "vanilo/framework": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Admin\\": "src"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit" : "^9.5",
        "laravel/legacy-factories": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
