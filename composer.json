{
    "name": "vanilo/admin",
    "description": "Admin Panel for the Vanilo E-commerce Framework",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/vanilophp/admin/issues"
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/framework": "^10.43|^11.0",
        "konekt/address": "^3.1",
        "konekt/appshell": "^4.0",
        "konekt/gears": "^1.12",
        "konekt/concord": "^1.15",
        "spatie/laravel-medialibrary": "^11.0",
        "vanilo/framework": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Admin\\": "src"
        }
    },
    "suggest": {
        "vanilo/advanced-pricing": "Use the Advanced Pricing Module for pricelist support"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit" : "^10.0",
        "laravel/legacy-factories": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
